{
  "name": "ng2-weather-icons",
  "version": "0.1.16",
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "test": "tsc && karma start",
    "postinstall": "typings install",
    "prepublish": "tsc",
    "tsc": "tsc",
    "typings": "typings",
    "htmlcopy": "copyfiles src/**/*.html dist",
    "csscopy": "copyfiles src/**/*.css dist",
    "scsscopy": "copyfiles src/**/*.scss dist",
    "lesscopy": "copyfiles src/**/*.less dist",
    "fontcopy": "copyfiles src/font/* dist",
    "dist": "rm -Rf dist && npm run tsc && npm run htmlcopy && npm run csscopy && npm run fontcopy && npm run lesscopy && npm run scsscopy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptitdam2001/ng2-weather-icons.git"
  },
  "author": {
    "name": "Damien Suhard",
    "email": "ptitdam2001@gmail.com"
  },
  "keywords": [
    "angular",
    "angular2",
    "icons",
    "weather"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ptitdam2001/ng2-weather-icons.git/issues"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@angular/common": "^2.0.0",
    "@angular/core": "^2.0.0",
    "lodash": "^4.17.2",
    "weather-icons": "github:erikflowers/weather-icons"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.46",
    "@types/lodash": "^4.14.57",
    "@types/node": "^6.0.66",
    "codelyzer": "^0.0.28",
    "copyfiles": "^1.0.0",
    "rxjs": "^5.2.0",
    "tslint": "^3.15.1",
    "typescript": "^2.2.0",
    "typings": "^1.0.4",
    "zone.js": "^0.8.4"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
